package com.damnacon.javaMavenSpringPgSMix.hs.data.dao;

public interface CustomerDao {

	/**
	 * Recherche d'un client ({@link Customer}) par son numero de permis
	 * 
	 * @param numberDrivingLicence le numero de permis du client
	 * @return le client (ou null si aucune correspondance)
	 */
	public Customer findById(int id);
	
	public void insert(Customer customer);
}